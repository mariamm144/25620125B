      <tuv lang="en-US">
        <seg>nori &lt;t0/&gt;sheets</seg>
      </tuv>
      <tuv lang="es" changeid="beatr" changedate="20230512T203352Z" creationid="beatr" creationdate="20230512T203352Z">
        <seg>hojas de&lt;t0/&gt;nori</seg>
      </tuv>
    </tu>
    <tu>
      <tuv lang="en-US">
        <seg>sushi rice &lt;t0/&gt;seasoning&lt;t1/&gt; &lt;t2/&gt;to&lt;t3/&gt; &lt;t4/&gt;drizzle</seg>
      </tuv>
      <tuv lang="es" changeid="beatr" changedate="20230512T203343Z" creationid="beatr" creationdate="20230512T203343Z">
        <seg>arroz sushi &lt;t0/&gt;sazonar&lt;t1/&gt; &lt;t2/&gt;a&lt;t3/&gt; &lt;t4/&gt;rociar</seg>
      </tuv>
    </tu>
    <tu>
      <tuv lang="en-US">
        <seg>your&lt;t0/&gt; &lt;t1/&gt;choice&lt;t2/&gt; &lt;t3/&gt;of&lt;t4/&gt; &lt;t5/&gt;fillings</seg>
      </tuv>
      <tuv lang="es" changeid="beatr" changedate="20230512T203407Z" creationid="beatr" creationdate="20230512T203407Z">
        <seg>tu&lt;t0/&gt; &lt;t1/&gt;elección&lt;t2/&gt; &lt;t3/&gt;de&lt;t4/&gt; &lt;t5/&gt;rellenos</seg>
      </tuv>
    </tu>
